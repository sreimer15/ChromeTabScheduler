{
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ], 
            "js": [
                "js/jquery-2.2.1.min.js"
            ]
        }
    ], 
    "background": {
        "scripts": [
            "background.js"
        ]
    }, 
    "short_name": "tabScheduler", 
    "name": "Chrome Tab Scheduler", 
    "version": "0.0.1.3", 
    "manifest_version": 2, 
    "browser_action": {
        "default_icon": "icon.png", 
        "default_popup": "popup.html"
    }, 
    "permissions": [
        "activeTab", 
        "storage", 
        "tabs", 
        "identity", 
        "identity.email", 
        "http://textance.herokuapp.com/title/", 
        "http://*/", 
        "https://*/", 
        "alarms"
    ]
}